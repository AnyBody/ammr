//        AnyRefFrame &s = Main.HumanModel.BodyModel.Trunk.Segments.SternalBodySeg.ManubriosternalJointNode;
AnyRefFrame &s = Main.HumanModel.BodyModel.Trunk.Segments.ThoraxSeg.SternalBody.Xiphoid_DiaphragmNodeR;
};
    }; // End of study
        
  }; //Main




