    AnyVar Vy = 0;
    AnyVar Vz = -mx/fyy;    
    
    AnyRefFrame& ref_VFPlate = .VFPlate_Right;
    
    AnyDrawLine Line = 
    {
        p0 = {.Vx, .Vy, .Vz};
