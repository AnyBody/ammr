AnyKinMeasureOrg TaloNavicularDorsiFlexion = {
  AnyFolder &Talus = ..Seg.Foot.Talus.TaloNavicularJoint;
  Talus ={
//    AnyDrawRefFrame drw = {};
    AnyRefNode RotNode={
      ARel={{....Sign*-1,0,0},{0,....Sign*-1,0},{0,0,1}};
//      AnyDrawRefFrame drw = {};
    };
  };
  AnyFolder &Navicular = ..Seg.Foot.Midfoot.Navicular.TaloNavicularJoint;
  Navicular ={
    AnyRefNode RotNode={
      ARel={{....Sign*-1,0,0},{0,....Sign*-1,0},{0,0,1}};
    };
  };
  AnyKinRotational Rot={
    AnyRefNode &TalusNode = ...Seg.Foot.Talus.TaloNavicularJoint.RotNode;
    AnyRefNode &NavicularNode = ...Seg.Foot.Midfoot.Navicular.TaloNavicularJoint.RotNode;
    Type=RotAxesAngles;
  };
  MeasureOrganizer={0};//z
}; 
AnyKinMeasureOrg TaloNavicularAbduction = {
  AnyKinRotational &Rot =.TaloNavicularDorsiFlexion.Rot; 
  MeasureOrganizer={1};//y
};
AnyKinMeasureOrg TaloNavicularEversion ={
  AnyKinRotational &Rot =.TaloNavicularDorsiFlexion.Rot; 
  MeasureOrganizer={2};//x
};




