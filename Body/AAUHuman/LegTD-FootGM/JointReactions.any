/* This file is used when there is no real muscles in the model
it adds reaction forces to the open dof in the leg*/


AnyFolder JointReactions = {
  
  
  AnyReacForce HipJoint={
    
    AnyKinMeasure &DOF = ..Jnt.Hip;
    
  };
  
  AnyReacForce  KneeJoint={
    AnyKinMeasure &DOF = ..Jnt.Knee;
    
  };
  
  
  AnyReacForce AnkleJnt={
    AnyKinMeasure &DOF = ..Jnt.Ankle;
  };
  
};// End of JointsLumbar