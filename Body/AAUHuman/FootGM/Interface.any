AnyKinMeasureOrg CalcaneoCuboidDorsiFlexion = {
  AnyFolder &Calcaneus = ..Seg.Calcaneus.CalcaneoCuboidJoint;
  Calcaneus ={
//    AnyDrawRefFrame drw = {};
    AnyRefNode RotNode={
      ARel={{....Sign*-1,0,0},{0,....Sign*-1,0},{0,0,1}};
//      AnyDrawRefFrame drw = {};
    };
  };
