  Cuboid ={
    AnyRefNode RotNode={
      ARel={{....Sign*-1,0,0},{0,....Sign*-1,0},{0,0,1}};
    };
  };
  AnyKinRotational Rot={
    AnyRefNode &CalcaneusNode = ...Seg.Calcaneus.CalcaneoCuboidJoint.RotNode;
    AnyRefNode &CuboidNode = ...Seg.Cuboid.CalcaneoCuboidJoint.RotNode;
    Type=RotAxesAngles;
  };
  MeasureOrganizer={0};//z
}; 
