      {Mat[1][1],Mat[1][2],Mat[1][0]},
      {Mat[2][1],Mat[2][2],Mat[2][0]}};


    AnySurfCylinder cyl = {
      Radius=vnorm(.sRel-.P3);
      Length= vnorm(.sRel-.P2);
      CapRatio=1.0;
//      AnyDrawParamSurf drw={RGB={0,1,0};};
    };

