  #include "WrapperCyl.any"
  Cylinder = {
    LengthScale = 2.3;
    ScaleFactor = (
      vnorm(...Seg.Humerus.Data.I_subscapularis_1_pos - ...Seg.Humerus.Data.I_subscapularis_5_pos) /
      vnorm(...Seg.Humerus.I_subscapularis_1.sRel - ...Seg.Humerus.I_subscapularis_5.sRel)
