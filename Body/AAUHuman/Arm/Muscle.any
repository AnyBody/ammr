AnyFolder Pectoralis_minor2_cyl={
  
  AnyFolder& pectoralis_ref =  ..ShoulderRef.O_pectoralis_minor_2;
  pectoralis_ref = {
    AnyRefNode PectoralisWrapAttachement = {
      ARel = RotMat(pi/2, y)*RotMat(-pi/2,x)*RotMat(pi/5*....Sign, x);
    };
