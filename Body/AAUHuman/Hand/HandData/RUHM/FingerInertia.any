// Bone and tissue density according to Abe et a. 1996
AnyVar BoneDensity = 1.9*1000;
AnyVar TissueDensity = 1.0*1000;

// Principal mass moments of intertia according to Wu et al. 2008
// Mass moment of interia of tissue
AnyVar I_y_t= (pi*TissueDensity)/4*((BoneMatrix[2][0]*BoneMatrix[2][1]*BoneMatrix[2][2])*(BoneMatrix[2][0]^2+BoneMatrix[2][1]^2)-(BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2])*(BoneMatrix[0][0]^2+BoneMatrix[0][1]^2));
AnyVar I_x_t= (pi*TissueDensity)/12*((BoneMatrix[2][0]*BoneMatrix[2][1]*BoneMatrix[2][2])*(3*BoneMatrix[2][0]^2+BoneMatrix[2][2]^2)-(BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2])*(3*BoneMatrix[0][0]^2+BoneMatrix[0][2]^2));
AnyVar I_z_t= (pi*TissueDensity)/12*((BoneMatrix[2][0]*BoneMatrix[2][1]*BoneMatrix[2][2])*(3*BoneMatrix[2][1]^2+BoneMatrix[2][2]^2)-(BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2])*(3*BoneMatrix[0][1]^2+BoneMatrix[0][2]^2));
// Mass moment of interia of bones
AnyVar I_y_b= (pi*BoneDensity)/4*((BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2])*(BoneMatrix[0][0]^2+BoneMatrix[0][1]^2)-(BoneMatrix[1][0]*BoneMatrix[1][1]*BoneMatrix[1][2])*(BoneMatrix[1][0]^2+BoneMatrix[1][1]^2));
AnyVar I_x_b= (pi*BoneDensity)/12*((BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2])*(3*BoneMatrix[0][0]^2+BoneMatrix[0][2]^2)-(BoneMatrix[1][0]*BoneMatrix[1][1]*BoneMatrix[1][2])*(3*BoneMatrix[1][0]^2+BoneMatrix[1][2]^2));
AnyVar I_z_b= (pi*BoneDensity)/12*((BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2])*(3*BoneMatrix[0][1]^2+BoneMatrix[0][2]^2)-(BoneMatrix[1][0]*BoneMatrix[1][1]*BoneMatrix[1][2])*(3*BoneMatrix[1][1]^2+BoneMatrix[1][2]^2));
// Superposition of mass moment of interia 
AnyVar I_xx=I_x_t+I_x_b;
AnyVar I_yy=I_y_t+I_y_b;
AnyVar I_zz=I_z_t+I_z_b;

//Calculation of the weight of the phalanx
AnyVar PhalanxMass = pi*(BoneMatrix[2][0]*BoneMatrix[2][1]-BoneMatrix[0][0]*BoneMatrix[0][1])*BoneMatrix[0][2]*TissueDensity+(pi*BoneMatrix[0][0]*BoneMatrix[0][1]*BoneMatrix[0][2]-BoneMatrix[1][0]*BoneMatrix[1][1]*BoneMatrix[1][2])*BoneDensity;