AnyKinEq DIP_PIP_Rhythm = 
{ 
  AnyKinMeasureLinComb RhythmMeasure = 
  {
    AnyRevoluteJoint &ref=  ..DIP;
    AnyKinMeasureFunComb1 FunComp = 
    {
      AnyKinMeasureOrg &Jnt = ...PIP;
      Functions = {&PIP_to_DIP_Function};    
      AnyFunPolynomial PIP_to_DIP_Function = 
      {
        // van Zwieten, Koos Jaap, et al. "An analytical expression for the DID PIP flexion interdependence in human fingers." (2015).
        // Paper graph fitted with a polynomial
        AnyFloat  p ={ -1.622e-12, 3.588e-10, 3.808e-08, -1.548e-05, 0.0012, -0.01627,  0.03657, 0.03706};
        PolyCoef = (pi/180)*{{p[7],p[6]*(180/pi),p[5]*(180/pi)^2,p[4]*(180/pi)^3,p[3]*(180/pi)^4,p[2]*(180/pi)^5,p[1]*(180/pi)^6,p[0]*(180/pi)^7}};
      };      
    }; 
    OutDim = 1;
    Coef = {{1,-1}}; 
  };
};
