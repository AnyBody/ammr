  ARel=RotMat(sRel,.P2.sRel,{0.0,0.0,0.0})*RotMat(0.5*pi,y);
  //    AnyDrawRefFrame drw={RGB={1,0,0};};
};

//unscaled second point on cyl axis
AnyRefNode P2 ={
  sRel= (0.1*{0.21,	-0.14	,..Sign*-0.79})+.P1.sRel;
  //    AnyDrawRefFrame drw={RGB={1,0,0};};
};

//unscaled point on cyl surf
