#ifndef GlueteusWrap
#define GlueteusWrap(NAME, NUM, SCALE) AnyRefNode NAME = \
  {\
      AnyVec3 origref = (NUM-1)/11*.GluteusMaximusSuperior1Node.sRel+\
                        (12-NUM)/11*.GluteusMaximusInferior6Node.sRel;\
      AnyMatrix p = {.AdductorMagnusDistal1Node.sRel*{{1,0,0},{0,1,0},{0,0,-1}}',\
                     .AdductorMagnusDistal1Node.sRel +\
                                (origref - .GluteusMaximusInferior6Node.sRel)*\
                                {{1,0,0},{0,1.3,0},{0,0,1}}',\
                                origref + {0,-0.015,0},\
                              .AdductorMagnusDistal1Node.sRel + {0,-0.005, 0} ,\
                               .AdductorMagnusMid1Node.sRel + {0,-0.005, 0} };\
      AnyFloat lengthscalefactor = 3.5;\
      AnyFloat radiusscalefactor=SCALE;\
      AnyMat33 transform = RotMat(iffun(eqfun(..Sign,1),0.0,pi),x);\
    };\
    NAME 
#endif

