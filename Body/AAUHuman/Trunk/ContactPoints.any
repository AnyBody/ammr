AnyFolder ContactPoints ={
  
  AnyFolder &refLumbar=.SegmentsLumbar;
  AnyFolder &refThorax=.SegmentsThorax;

  refLumbar.PelvisSeg={

    AnyRefNode PosteriorRContactNode = {sRel = .Scale(.Data.PosteriorRContactNode_pos);};    
    AnyRefNode PosteriorLContactNode = {sRel = .Scale(.Data.PosteriorLContactNode_pos);};    
    
    AnyRefNode Seat1RContactNode={sRel = .Scale(.Data.Seat1RContactNode_pos);};    
    AnyRefNode Seat1LContactNode={sRel = .Scale(.Data.Seat1LContactNode_pos);};    
    
    AnyRefNode Seat2RContactNode={sRel = .Scale(.Data.Seat2RContactNode_pos);};    
    AnyRefNode Seat2LContactNode={sRel = .Scale(.Data.Seat2LContactNode_pos);};    
    
    AnyRefNode Seat2MidContactNode={sRel=0.5*(.Seat2RContactNode.sRel+.Seat2LContactNode.sRel);}; 

    AnyRefNode Seat3RContactNode={sRel = .Scale(.Data.Seat3RContactNode_pos);};    
    AnyRefNode Seat3LContactNode={sRel = .Scale(.Data.Seat3LContactNode_pos);};    
    
    AnyRefNode Seat4RContactNode={sRel = .Scale(.Data.Seat4RContactNode_pos);};    
    AnyRefNode Seat4LContactNode={sRel = .Scale(.Data.Seat4LContactNode_pos);};    
    
  };     
  

  refLumbar.SacrumSeg={    
    AnyRefNode SacrumContactNode = {sRel= .Scale(.Data.SacrumContactNode_pos);};
    AnyRefNode SacrumRContactNode = {sRel= .Scale(.Data.SacrumRContactNode_pos);};
    AnyRefNode SacrumLContactNode = {sRel= .Scale(.Data.SacrumLContactNode_pos);};
    
  };

  refLumbar.L5Seg={
    AnyRefNode L5RContactNode = {sRel= .Scale(.Data.L5RContactNode_pos);};
    AnyRefNode L5LContactNode = {sRel= .Scale(.Data.L5RContactNode_pos);};
  };
  
  refLumbar.L4Seg={
    AnyRefNode L4RContactNode = {sRel= .Scale(.Data.L4RContactNode_pos);};
    AnyRefNode L4LContactNode = {sRel= .Scale(.Data.L4RContactNode_pos);};
  };
  
  
  refLumbar.L3Seg={
    AnyRefNode L3RContactNode = {sRel= .Scale(.Data.L3RContactNode_pos);};
    AnyRefNode L3LContactNode = {sRel= .Scale(.Data.L3RContactNode_pos);};
  };
  
  refLumbar.L2Seg={
    AnyRefNode L2RContactNode = {sRel= .Scale(.Data.L2RContactNode_pos);};
    AnyRefNode L2LContactNode = {sRel= .Scale(.Data.L2RContactNode_pos);};
  };
  
  refLumbar.L1Seg={
    AnyRefNode L1RContactNode = {sRel= .Scale(.Data.L1RContactNode_pos);};
    AnyRefNode L1LContactNode = {sRel= .Scale(.Data.L1RContactNode_pos);};
  };

  refThorax.SkullSeg={ 
    AnyRefNode SkullContactNode = {sRel= .Scale(.Data.SkullContactNode_pos);};
  };
  
};
