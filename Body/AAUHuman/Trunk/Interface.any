AnyKinMeasureOrg PelvisPosX ={
  AnyKinLinear lin ={
    AnyRefFrame &ref2 =....Trunk.SegmentsLumbar.PelvisSeg;
  };
  MeasureOrganizer={0};
};
AnyKinMeasureOrg PelvisPosY ={
  AnyKinLinear &ref=.PelvisPosX.lin;
  MeasureOrganizer={1};
};
AnyKinMeasureOrg PelvisPosZ ={
  AnyKinLinear &ref=.PelvisPosX.lin;
  MeasureOrganizer={2};
};


AnyKinMeasureOrg PelvisRotX ={
  AnyKinRotational rot ={
    AnyRefFrame &ref2 =....Trunk.SegmentsLumbar.PelvisSeg.AnatomicalFrame;
    Type=RotAxesAngles;
  };
  MeasureOrganizer={2};
};
AnyKinMeasureOrg PelvisRotY ={
  AnyKinRotational &ref=.PelvisRotX.rot;
  MeasureOrganizer={1};
};
AnyKinMeasureOrg PelvisRotZ ={
  AnyKinRotational &ref=.PelvisRotX.rot;
  MeasureOrganizer={0};
};


