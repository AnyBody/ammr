AnySeg R1Seg = {

  AnyFolder &StdPar=...ModelParameters;
  
  // This file includes the trunk specifc scaling function
  // and anatomical reference system
  AnyFunTransform3D &GeomScale = ...Scaling.GeometricalScaling.RibCage.ScaleFunction;
  #include "TrunkScalingAndAnatomicalFrame.any"
  AnyRefNode MoCapMarkerFrameAMMR24 = {}; ///< For backwards compatability with AMMR 2.4 MoCap Marker protocols
    
  r0=..T1Seg.r0 +
     ..T1Seg._SIDE_.sRel*..T1Seg.Axes0'  +
     ..T1Seg._SIDE_.CTCVJntNode.sRel*..T1Seg._SIDE_.ARel'*..T1Seg.Axes0'-
     CTCVJntNode.sRel*Axes0'; 
  
  // Inertia properties for the segment
  UseInertiaObjects = On;
  segmental_inertia_matrix inertiaObject() = {
    sCoM = (.CT.sRel + .CC.sRel)/2;
  };
  
  AnyRefNode CT = { sRel = .Scale(.StdPar.R1._SIDE_.CT_pos); };
  AnyRefNode CC = { sRel = .Scale(.StdPar.R1._SIDE_.CC_pos); };
  AnyRefNode CV = { sRel = .Scale(.StdPar.R1._SIDE_.CV_pos); };   
  AnyRefNode R1R2IC = { sRel = .Scale(.StdPar.R1._SIDE_.R1R2IC_pos); };
  AnyRefNode &CTCVJntNode = CV;

  AnyRefNode CTsup = { sRel = .Scale(.StdPar.R1._SIDE_.CTsup_pos); };
  AnyRefNode CTinf = { sRel = .Scale(.StdPar.R1._SIDE_.CTinf_pos); };

  AnyDrawSurf Costa1 = {
    FileName = ....STL.FilenameR1Seg;
    RGB = Main.DrawSettings.Colors.Segments;
    ScaleXYZ = {1.0, 1.0, 1.0 * ..Sign};
    AnyFunTransform3D &Scale = .Scale;
  };  
}; // End of R1Seg

