  AnyFolder &MusRightRef = ....BodyModel.Right.ShoulderArm.Mus;
  AnyFolder &MusLeftRef = ....BodyModel.Left.ShoulderArm.Mus;


  
  
  
  
 AnyFixedRefFrame ground = {
   AnyRefNode node = { 
