      AnyKinMeasure& ref1 = .......BodyModel.Interface.Left.HipFlexion;
      AnyKinMeasure& ref2 = .......BodyModel.Interface.Left.HipAbduction;
      AnyKinMeasure& ref3 = .......BodyModel.Interface.Left.HipExternalRotation;
    };
  };
