#class_template segmental_inertia_matrix(
    __CLASS__ = AnyInertiaMatrix,
){
    Mass ??= 1e-5;
    sCoM ??= {0,0,0};
    JaboutCoMOnOff ??= .JaboutCoMOnOff;
    Jii ??= {0,0,0};
    Jij ??= {0,0,0};
};

#class_template segmental_inertia_surface(
    __CLASS__ = AnyInertiaSolid,
    SURFACE,
    AnyFunTransform3D &scale_fun,
){
    Density ??= 1850; // AKHTARUZZAMAN & KUKO≈ÅOWICZ, 2020 - Tbl. 1 states Bone as 1850 kg/m3 

    AnySurfSTL surface = {
        FileName = SURFACE;
        AnyFunTransform3D &scale = .scale_fun;
    };
};