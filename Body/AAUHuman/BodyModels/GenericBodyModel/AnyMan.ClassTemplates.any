


#class_template Template_Anthropometrics_XYZ 
{
  
    #var AnyVar BMI = BodyMass/(BodyHeight^2);
    ///Estimation from Frankenfield et al. (2001) valid for men
    #var AnyVar FatPercent = (-0.09 + 0.0149*BMI - 0.00009*BMI^2)*100;
    
    #var AnyVar BodyMass = DesignVar(75) ;
    #var AnyVar BodyHeight = DesignVar(180 /100);
    
    AnyFolder SegmentMasses = {
    
    //Segment masses in kg from Winter ("Biomechanics and motor control of human movement." David A. Winter)
    #var AnyVar Lumbar = 0.139*.BodyMass; // T12-L1 to L5-Sacrum
    #var AnyVar Thorax = 0.1894*.BodyMass; // C7-T1 to T12-L1 (modified from 0.216 winter to separate scapula)
    #var AnyVar Pelvis = 0.142*.BodyMass;
    #var AnyVar Head = 0.081*.BodyMass; // head and cervical
    
    AnyFolder Right = 
    {
        #var AnyVar Clavicle = 0.0133*..BodyMass; //< Combined mass of shoulder muscles and scapula/clavicula bones.
        #var AnyVar UpperArm = 0.028*..BodyMass;
        #var AnyVar LowerArm = 0.016*..BodyMass;
        #var AnyVar Hand = 0.006*..BodyMass;
        #var AnyVar Thigh = 0.1*..BodyMass;
        #var AnyVar Shank = 0.0465*..BodyMass;
        #var AnyVar Talus = 0.2*0.0145*..BodyMass; //20% of total foot (from bone volume ratio)
        #var AnyVar Foot =  0.8*0.0145*..BodyMass; //80% of total foot (from bone volume ratio)
    };
    
    AnyFolder Left = 
    {
        #var AnyVar Clavicle = 0.0133*..BodyMass;
        #var AnyVar UpperArm = 0.028*..BodyMass;
        #var AnyVar LowerArm = 0.016*..BodyMass;
        #var AnyVar Hand = 0.006*..BodyMass;
        #var AnyVar Thigh = 0.1*..BodyMass;
        #var AnyVar Shank = 0.0465*..BodyMass;
        #var AnyVar Talus = 0.2*0.0145*..BodyMass; //20% of total foot (from bone volume ratio)
        #var AnyVar Foot =  0.8*0.0145*..BodyMass; //80% of total foot (from bone volume ratio)
    };   
  };

  AnyFolder SegmentScaleFactors = 
  {

    AnyFolder Pelvis = { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
    AnyFolder Trunk = { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
    AnyFolder Head = { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
    AnyFolder Lumbar = { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };

    AnyFolder Right = 
    {
        AnyFolder Clavicula =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Scapula =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Humerus =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Radius =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Ulna  =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Hand =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Foot =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Shank =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Thigh =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
    };
    AnyFolder Left = 
    {
        AnyFolder Clavicula =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Scapula =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Humerus =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Radius =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Ulna  =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Hand =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Foot =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Shank =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
        AnyFolder Thigh =  { #var AnyFloat LengthScale = DesignVar(1); #var AnyFloat DepthScale = DesignVar(1); #var AnyFloat WidthScale = DesignVar(1); };
    };
  };

};









