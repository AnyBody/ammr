  #if BM_LEG_RIGHT & (BM_LEG_MODEL == _LEG_MODEL_TLEM_| BM_LEG_MODEL == _LEG_MODEL_TLEM2_) & (BM_LEG_MUSCLES_RIGHT == _MUSCLES_3E_HILL_)
