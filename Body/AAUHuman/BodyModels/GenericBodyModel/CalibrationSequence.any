
//This file controls which calibrations to be made on the model 
AnyFolder Calibration={  
  #if RIGHT_LEG_TD + RIGHT_LEG_TD_MUS_3E == 2
    #if BM_CALIBRATION_TYPE == _1PARAMETER_CALIBRATION_
      #include "../../LegTLEM/Calibration/CalibrationSequenceRight.any"
    #endif
    #if BM_CALIBRATION_TYPE == _2PARAMETER_CALIBRATION_
      #include "../../LegTLEM/Calibration/CalibrationSequenceRight2Par.any"
    #endif
    #define RIGHT_LEG_TD_CALIBRATION 1
    #else
    #define RIGHT_LEG_TD_CALIBRATION 0
  #endif
  
  #if LEFT_LEG_TD + LEFT_LEG_TD_MUS_3E == 2
    #if BM_CALIBRATION_TYPE == _1PARAMETER_CALIBRATION_
      #include "../../LegTLEM/Calibration/CalibrationSequenceLeft.any"  
    #endif
    #if BM_CALIBRATION_TYPE == _2PARAMETER_CALIBRATION_
      #include "../../LegTLEM/Calibration/CalibrationSequenceLeft2Par.any"  
    #endif
    #define LEFT_LEG_TD_CALIBRATION 1
    #else
    #define LEFT_LEG_TD_CALIBRATION 0
  #endif
  
  #if RIGHT_LEG + RIGHT_LEG_MUS_3E == 2
  #include "../../Leg/CalibrationSequenceRight.any"
  #define RIGHT_LEG_CALIBRATION 1
  #else
  #define RIGHT_LEG_CALIBRATION 0
  #endif
  
  #if LEFT_LEG + LEFT_LEG_MUS_3E == 2
  #include "../../Leg/CalibrationSequenceLeft.any"  
  #define LEFT_LEG_CALIBRATION 1
  #else
  #define LEFT_LEG_CALIBRATION 0
  #endif
  
  #if RIGHT_ARM + RIGHT_ARM_MUS_3E == 2
    #if BM_TRUNK_CERVICAL_MUSCLES != CONST_MUSCLES_NONE
    #include "../../Arm/CalibrationSequenceNeckRight.any" 
    #else
    #include "../../Arm/CalibrationSequenceRight.any"
    #endif
  #define RIGHT_ARM_CALIBRATION 1
  #else
  #define RIGHT_ARM_CALIBRATION 0
  #endif
  
  #if LEFT_ARM + LEFT_ARM_MUS_3E == 2
    #if BM_TRUNK_CERVICAL_MUSCLES != CONST_MUSCLES_NONE
    #include "../../Arm/CalibrationSequenceNeckLeft.any"  
    #else
    #include "../../Arm/CalibrationSequenceLeft.any"  
    #endif
  #define LEFT_ARM_CALIBRATION 1
  #else
  #define LEFT_ARM_CALIBRATION 0
  #endif

  #if (BM_TRUNK_LUMBAR_LIGAMENTS == ON)
    #define LUMBAR_LIG_CALIBRATION 1
    #include "../../Trunk/CalibrationSequence.any"  
  #else
    #define LUMBAR_LIG_CALIBRATION 0
  #endif
  
  #if RIGHT_LEG_TD_CALIBRATION + LEFT_LEG_TD_CALIBRATION + RIGHT_LEG_CALIBRATION + LEFT_LEG_CALIBRATION + RIGHT_ARM_CALIBRATION + LEFT_ARM_CALIBRATION + LUMBAR_LIG_CALIBRATION == 0
  AnyOperationDummy CalibrationSequence ={};
  #else
  AnyOperationSequence CalibrationSequence ={};
  #endif

  #undef RIGHT_LEG_TD_CALIBRATION
  #undef LEFT_LEG_TD_CALIBRATION
  #undef RIGHT_LEG_CALIBRATION
  #undef LEFT_LEG_CALIBRATION
  #undef RIGHT_ARM_CALIBRATION
  #undef LEFT_ARM_CALIBRATION
  #undef LUMBAR_LIG_CALIBRATION
  
};
