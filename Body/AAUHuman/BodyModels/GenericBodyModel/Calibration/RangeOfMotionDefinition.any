      AnyVar MaxGlenohumeralExtensionAngle ??= .....BodyModel.Right.ShoulderArm.Data.RangeOfMotion.Shoulder.MaxGlenohumeralExtensionAngle;
      AnyVar MaxGlenohumeralAbductionAngle ??= .....BodyModel.Right.ShoulderArm.Data.RangeOfMotion.Shoulder.MaxGlenohumeralAbductionAngle;
