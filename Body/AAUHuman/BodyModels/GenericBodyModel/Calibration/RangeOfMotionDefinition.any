  Trunk = {
    AnyVar MaxSkullThoraxFlexionAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MaxSkullThoraxFlexionAngle;
    AnyVar MaxSkullThoraxExtensionAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MaxSkullThoraxExtensionAngle;
    AnyVar MaxSkullThoraxLateralBendingAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MaxSkullThoraxLateralBendingAngle;
    AnyVar MinSkullThoraxLateralBendingAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MinSkullThoraxLateralBendingAngle;
    AnyVar MaxSkullThoraxAxialRotationAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MaxSkullThoraxAxialRotationAngle;
    AnyVar MinSkullThoraxAxialRotationAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MinSkullThoraxAxialRotationAngle;
    AnyVar MaxPelvisThoraxFlexionAngle ??= ...BodyModel.Trunk.Data.RangeOfMotion.MaxPelvisThoraxFlexionAngle;
