      AnyVar MinFlexionInAdduction ??= .....BodyModel.Right.Leg.Data.RangeOfMotion.Hip.MinFlexionInAdduction;
  
      // Hip positions
      AnyFloat Neutral ??= .....BodyModel.Right.Leg.Data.RangeOfMotion.Hip.Neutral; 
      AnyFloat InternallyRotated ??= .....BodyModel.Right.Leg.Data.RangeOfMotion.Hip.InternallyRotated;
