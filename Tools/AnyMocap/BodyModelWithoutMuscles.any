        ObjSearchRecursive("Main.HumanModel.BodyModel.Trunk.Abdominal", "*", "AnyMechObject"),
        ObjSearchRecursive("Main.HumanModel.BodyModel.Trunk.Buckle", "*", "AnyMechObject"),
        ObjSearchRecursive("Main.HumanModel.BodyModel.Right.ShoulderArm.Muscles", "*", "AnyMechObject"),
        ObjSearchRecursive("Main.HumanModel.BodyModel.Left.ShoulderArm.Muscles", "*", "AnyMechObject"),
     );
    };
      
    AnyFolder& SelectedOutput= Main.HumanModel.BodyModel.SelectedOutput;
    
    AnyFolder Trunk = {
      AnyFolder &Segments = Main.HumanModel.BodyModel.Trunk.Segments;
      AnyFolder &JointsLumbar = Main.HumanModel.BodyModel.Trunk.Joints;

    };
    
    AnyFolder Right = {
      #if BM_ARM_RIGHT
      AnyFolder ShoulderArm = {
        AnyFolder &Seg = Main.HumanModel.BodyModel.Right.ShoulderArm.Segments;
        AnyFolder &Jnt = Main.HumanModel.BodyModel.Right.ShoulderArm.Joints;
        #if BM_ARM_DELTOID_WRAPPING == _DELTOID_WRAPPING_RAKE_
        AnyFolder &rake = Main.HumanModel.BodyModel.Right.ShoulderArm.Muscles.ArtificialRake;
        #else
        AnyFolder &DeltoidWrappingPosteriorRef = Main.HumanModel.BodyModel.Right.ShoulderArm.Muscles.DeltoidWrappingPosterior;
        AnyFolder &DeltoidWrappingLateralRef = Main.HumanModel.BodyModel.Right.ShoulderArm.Muscles.DeltoidWrappingLateral;
        AnyFolder &DeltoidWrappingAnteriorRef = Main.HumanModel.BodyModel.Right.ShoulderArm.Muscles.DeltoidWrappingAnterior;
        #endif
      };
      #endif
      #if BM_LEG_RIGHT
      AnyFolder Leg = {
        AnyFolder &Seg = Main.HumanModel.BodyModel.Right.Leg.Seg;
        AnyFolder &Jnt = Main.HumanModel.BodyModel.Right.Leg.Jnt;
      };
      #endif
    };
    AnyFolder Left = {
      #if BM_ARM_LEFT
      AnyFolder ShoulderArm = {
        AnyFolder &Seg = Main.HumanModel.BodyModel.Left.ShoulderArm.Segments;
        AnyFolder &Jnt = Main.HumanModel.BodyModel.Left.ShoulderArm.Joints;
        #if BM_ARM_DELTOID_WRAPPING == _DELTOID_WRAPPING_RAKE_
        AnyFolder &rake = Main.HumanModel.BodyModel.Left.ShoulderArm.Muscles.ArtificialRake;
        #else
