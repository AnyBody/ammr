    };
    #endif
    #if RIGHT_FOOT_LENGTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_RightFootLength = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_RightFootLength; };
