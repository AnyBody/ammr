    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_HeadHeight = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_HeadHeight; };
    #endif

    #if CREATE_ALL_DESVARS | HEAD_WIDTH
    AnyDesVar NAME_PARA_OPT##_HeadWidth = {
      Val = SEGMENTS_LENGTHS.HeadWidth;
      Min = 0.05;
      Max = 0.5;
    };
    #endif
    #if HEAD_WIDTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_HeadWidth = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_HeadWidth; };
    #endif

    #if CREATE_ALL_DESVARS | HEAD_DEPTH
    AnyDesVar NAME_PARA_OPT##_HeadDepth = {
      Val = SEGMENTS_LENGTHS.HeadDepth;
      Min = 0.05;
      Max = 0.5;
    };
    #endif
    #if HEAD_DEPTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_HeadDepth = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_HeadDepth; };
    #endif

    #if CREATE_ALL_DESVARS | NECK_LENGTH
    AnyDesVar NAME_PARA_OPT##_NeckLength = {
      Val = SEGMENTS_LENGTHS.NeckLength;
      Min = 0.05;
      Max = 0.5;
    };
    #endif
    #if NECK_LENGTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_NeckLength = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_NeckLength; };
    #endif


    #if CREATE_ALL_DESVARS | TRUNK_HEIGHT
    AnyDesVar NAME_PARA_OPT##_TrunkHeight = {
      Val = SEGMENTS_LENGTHS.TrunkHeight;
      Min = 0.1;
      Max = 1.5;
    };
    #endif
    #if TRUNK_HEIGHT
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_TrunkHeight = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_TrunkHeight; };
    #endif

    #if CREATE_ALL_DESVARS | TRUNK_WIDTH
    AnyDesVar NAME_PARA_OPT##_TrunkWidth = {
      Val = SEGMENTS_LENGTHS.TrunkWidth;
      Min = 0.1;
