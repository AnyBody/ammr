    #if TRUNK_WIDTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_TrunkWidth = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_TrunkWidth; };
    #endif

    #if CREATE_ALL_DESVARS | TRUNK_DEPTH
    AnyDesVar NAME_PARA_OPT##_TrunkDepth = {
      Val = SEGMENTS_LENGTHS.TrunkDepth;
      Min = 0.1;
      Max = 1.5;
    };
    #endif
    #if TRUNK_DEPTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_TrunkDepth = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_TrunkDepth; };
    #endif

    #if CREATE_ALL_DESVARS | RIGHT_THIGH_LENGTH
    AnyDesVar NAME_PARA_OPT##_RightThighLength = {
      Val = SEGMENTS_LENGTHS.Right.ThighLength;
      Min = 0.1;
      Max = 1;
    };
    #endif
    #if RIGHT_THIGH_LENGTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_RightThighLength = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_RightThighLength; };
    #endif

    #if CREATE_ALL_DESVARS | LEFT_THIGH_LENGTH
    AnyDesVar NAME_PARA_OPT##_LeftThighLength = {
      Val = SEGMENTS_LENGTHS.Left.ThighLength;
      Min = 0.1;
      Max = 1;
    };
    #endif
    #if LEFT_THIGH_LENGTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_LeftThighLength = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_LeftThighLength; };
    #endif

    #if CREATE_ALL_DESVARS | (RIGHT_THIGH_LENGTH & LEFT_THIGH_LENGTH & LEFT_RIGHT_SYMMETRY)
    AnyDesMeasure NAME_PARA_OPT##_ThighLengthSymmetry = {
      Val = .NAME_PARA_OPT##_LeftThighLength.Val - .NAME_PARA_OPT##_RightThighLength.Val;
      Type = EqualToZero;
    };
    #endif
    #if RIGHT_THIGH_LENGTH & LEFT_THIGH_LENGTH & LEFT_RIGHT_SYMMETRY
    PARAMETER_OPT_STUDY = { AnyDesMeasure& NAME_PARA_OPT##_ThighLengthSymmetry = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_ThighLengthSymmetry; };
    #endif

    #if CREATE_ALL_DESVARS | RIGHT_SHANK_LENGTH
    AnyDesVar NAME_PARA_OPT##_RightShankLength = {
      Val = SEGMENTS_LENGTHS.Right.ShankLength;
      Min = 0.1;
      Max = 1;
    };
    #endif
    #if RIGHT_SHANK_LENGTH
    PARAMETER_OPT_STUDY = { AnyDesVar& NAME_PARA_OPT##_RightShankLength = Main.ModelSetup.ParameterIdentification.NAME_PARA_OPT##_RightShankLength; };
    #endif

    #if CREATE_ALL_DESVARS | LEFT_SHANK_LENGTH
    AnyDesVar NAME_PARA_OPT##_LeftShankLength = {
