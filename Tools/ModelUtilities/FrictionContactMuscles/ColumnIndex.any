      Line.RGB = ..DrawRef.Support.Lin.RGB;
      Line.Thickness = 10*..DrawRef.Support.Lin.Thickness;
      Line.End.Thickness = 0;
      Line.End.Length = 0;

    };

    AnyDrawVector DrawText =     {
