AnyRecruitedActuator RecruitedReaction1 = {
  AnyKinLinear Lin={
    Ref=0;
    RefFrames = {
      repmat(6,&..ref1),
      {
        &..ref2.Calcaneus,
        &..ref2.DistalPhalange1,
        &..ref2.DistalPhalange2,
        &..ref2.DistalPhalange3,
        &..ref2.DistalPhalange4,
        &..ref2.DistalPhalange5,
      }
    };
  };
  AnyKinRotational Rot1={
    AnyRefFrame &ref1=..ref1;
    AnyRefFrame &ref2=..ref2.Calcaneus;
    Type=RotAxesAngles;
  };
  AnyKinRotational Rot2={
    AnyRefFrame &ref1=..ref1;
    AnyRefFrame &ref2=..ref2.DistalPhalange1;
    Type=RotAxesAngles;
  };
  AnyKinRotational Rot3={
    AnyRefFrame &ref1=..ref1;
    AnyRefFrame &ref2=..ref2.DistalPhalange2;
    Type=RotAxesAngles;
  };
  AnyKinRotational Rot4={
    AnyRefFrame &ref1=..ref1;
    AnyRefFrame &ref2=..ref2.DistalPhalange3;
    Type=RotAxesAngles;
  };
  AnyKinRotational Rot5={
    AnyRefFrame &ref1=..ref1;
    AnyRefFrame &ref2=..ref2.DistalPhalange4;
    Type=RotAxesAngles;
  };
  AnyKinRotational Rot6={
    AnyRefFrame &ref1=..ref1;
    AnyRefFrame &ref2=..ref2.DistalPhalange5;
    Type=RotAxesAngles;
  };
  Type = NonPositive; 
  Strength=.Strength;
  SET_DEFAULT_ACTUATOR_VOLUME;        
};


